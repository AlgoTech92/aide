{
  "name": "codestory-authentication",
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "codestory",
  "license": "MIT",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.41.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "api": "none",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [],
  "capabilities": {
    "virtualWorkspaces": true
  },
  "contributes": {
    "commands": [
      {
        "command": "codestory-authentication.login",
        "title": "Login to CodeStory account"
      }
    ],
    "authentication": [
      {
        "label": "CodeStory",
        "id": "codestory"
      }
    ]
  },
  "main": "./out/extension.js",
  "browser": "./dist/browser/extension.js",
  "scripts": {
    "compile": "gulp compile-extension:codestory-authentication",
    "compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
    "watch": "gulp watch-extension:codestory-authentication",
    "watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose",
    "vscode:prepublish": "npm run compile"
  },
  "dependencies": {
    "node-fetch": "2.6.7",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^10.0.0"
  }
}
