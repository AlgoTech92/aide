{
  "name": "codestoryai",
  "displayName": "CodeStory.AI",
  "description": "CodeStory is an AI assistant to remove all context switch costs for code reviews.",
  "version": "0.6.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "webview.showChatView",
        "title": "CodeStory: Show"
      }
    ]
  },
  "scripts": {
    "install:all": "yarn install && cd webview-ui && yarn install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "gulp compile-extension:codestory",
    "watch": "gulp watch-extension:codestory",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@estruyf/vscode": "^1.1.0",
    "winston": "^3.10.0",
    "winston-vscode": "^1.0.0"
  }
}
