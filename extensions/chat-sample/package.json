{
  "name": "chat-sample",
  "publisher": "vscode-samples",
  "displayName": "Copilot Chat Sample",
  "description": "Sample chat extension, a trusty cat tutor that will can teach you computer science topics.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "enabledApiProposals": [
    "chatParticipant",
    "chatParticipantAdditions",
    "defaultChatParticipant",
    "chatVariableResolver",
    "languageModels"
  ],
  "contributes": {
    "chatParticipants": [
      {
        "id": "chat-sample.alphaCat",
        "name": "alphacat",
        "isDefault": true,
        "description": "Meow! I'm the boss, get it?",
        "commands": [
          {
            "name": "teach",
            "description": "Pick at random a computer science concept then explain it in purfect way of a cat"
          },
          {
            "name": "play",
            "description": "Do whatever you want, you are a cat after all"
          }
        ]
      },
      {
        "id": "chat-sample.cat",
        "name": "cat",
        "description": "Meow! What can I teach you?",
        "isSticky": true
      }
    ],
    "commands": [
      {
        "command": "cat.namesInEditor",
        "title": "Use Cat Names in Editor"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "gulp compile-extension:chat-sample",
    "watch": "gulp watch-extension:chat-sample"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/vscode": "1.82.0",
    "eslint": "^7.22.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
